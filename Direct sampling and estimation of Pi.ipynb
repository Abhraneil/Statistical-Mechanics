    "import random\n",
    "\n",
    "\n",
    "n_hits = 0\n",
    "n_trials = 100000000\n",
    "\n",
    "for i in range(n_trials):\n",
    "    x,y = random.uniform(-1.0,1.0) , random.uniform(-1.0,1.0)\n",
    "    if x**2 + y**2 < 1:\n",
    "        n_hits += 1\n",
    "print(4*n_hits/float(n_trials))      \n",
    "        \n",
    "        \n",
    "    "
